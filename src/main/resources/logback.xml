<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true">

  <!-- 输出到控制台 -->
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss} [%level] - %m%n</pattern>
    </encoder>
  </appender>

  <!-- 配置好前面对应的appender -->
  <root level="INFO">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
